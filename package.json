{
  "name": "building-derivations",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist && tsr generate",
    "build": "npm-run-all --parallel build:*",
    "build:tsc": "tsc",
    "build:esbuild": "node esbuild/prod.mjs",
    "build:css": "tailwindcss -i src/styles.css -o dist/styles.css --minify",
    "start": "http-server dist/ --port 8000 -P http://localhost:8000?",
    "predev": "rimraf dist",
    "dev": "npm run build && npm-run-all --parallel dev:*",
    "dev:tsc": "tsc -w",
    "dev:esbuild": "node esbuild/dev.mjs",
    "dev:route": "tsr watch",
    "dev:css": "tailwindcss -i src/styles.css -o dist/styles.css --watch",
    "pretest": "npm run cov:build",
    "test": "start-server-and-test 'npm run start' http://localhost:8080 cov",
    "posttest": "npm run report:combined",
    "cov": "npm-run-all --parallel cov:frontend cov:backend",
    "precov:build": "rimraf dist && tsr generate",
    "cov:build": "npm-run-all --parallel cov:build:*",
    "cov:build:esbuild": "node esbuild/coverage.mjs",
    "cov:build:css": "tailwindcss -i src/styles.css -o dist/styles.css",
    "postcov:build": "nyc instrument --in-place dist",
    "cov:frontend": "cypress run --config baseUrl=http://localhost:8080",
    "cov:backend": "jest --coverage",
    "mkdir:reports": "mkdir reports || true",
    "precopy:reports": "npm run mkdir:reports",
    "copy:reports": "cp coverage/coverage-final.json reports/from-cypress.json && cp jest-coverage/coverage-final.json reports/from-jest.json",
    "precombine:reports": "npm run copy:reports && mkdir .nyc_output || true",
    "combine:reports": "npx nyc merge reports && mv coverage.json .nyc_output/out.json",
    "prereport:combined": "npm run combine:reports",
    "report:combined": "npx nyc report --reporter lcov --reporter text --report-dir coverage",
    "test:dev": "npm-run-all --parallel test:dev:*",
    "test:dev:frontend": "cypress run --config baseUrl=http://localhost:8000",
    "test:dev:backend": "jest",
    "cy:open": "cypress open --config baseUrl=http://localhost:8000",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "Justin Keung",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@cypress/code-coverage": "^3.13.12",
    "@eslint/js": "^9.17.0",
    "@types/jest": "^29.5.14",
    "@types/lodash.isequal": "^4.5.8",
    "@types/node": "^22.10.2",
    "@types/react-dom": "^19.0.2",
    "esbuild": "^0.25.0",
    "esbuild-runner": "^2.2.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.11",
    "npm-run-all": "^4.1.5",
    "nyc": "^17.1.0",
    "prettier": "3.4.2",
    "start-server-and-test": "^2.0.10",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-separator": "^1.1.6",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-toggle": "^1.1.6",
    "@radix-ui/react-toggle-group": "^1.1.7",
    "@radix-ui/react-tooltip": "^1.2.6",
    "@react-hookz/web": "^25.1.1",
    "@tanstack/react-router": "^1.120.3",
    "@tanstack/react-router-devtools": "^1.120.3",
    "@tanstack/router-cli": "^1.120.3",
    "better-react-mathjax": "^2.0.4-beta1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cypress": "^14.0.3",
    "http-server": "^14.1.1",
    "lodash.isequal": "^4.5.0",
    "lucide-react": "^0.468.0",
    "parjs": "^1.3.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-resizable-panels": "^3.0.2",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.4"
  },
  "lint-staged": {
    "*/**/*.{mjs,js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{mjs,js,jsx,ts,tsx}": "prettier --write",
    "**/*.{css,md,yml,json}": "prettier --write"
  }
}
