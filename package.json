{
  "name": "building-derivations",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:tsc": "tsc -w",
    "dev:esbuild": "esbuild src/index.tsx src/index.html --bundle --loader:.html=copy --outdir=dist --servedir=dist --watch",
    "dev:css": "tailwindcss -i src/index.css -o dist/index.css --watch",
    "build": "npm-run-all --parallel build:*",
    "build:tsc": "tsc",
    "build:esbuild": "esbuild src/index.tsx src/index.html --bundle --loader:.html=copy --outdir=dist --minify",
    "build:css": "tailwindcss -i src/index.css -o dist/index.css --minify",
    "start": "http-server dist/",
    "test": "jest",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Justin Keung",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/jest": "^29.5.14",
    "@types/lodash.isequal": "^4.5.8",
    "@types/node": "^22.10.2",
    "@types/react-dom": "^19.0.2",
    "esbuild": "^0.25.0",
    "esbuild-runner": "^2.2.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-slot": "^1.1.1",
    "better-react-mathjax": "^2.0.4-beta1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cypress": "^14.0.3",
    "http-server": "^14.1.1",
    "lodash.isequal": "^4.5.0",
    "lucide-react": "^0.468.0",
    "parjs": "^1.3.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "typescript-parsec": "^0.3.4"
  },
  "lint-staged": {
    "*/**/*.{mjs,js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*/**/*.{css,md,yml}": "prettier --write"
  }
}
